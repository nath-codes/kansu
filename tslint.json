{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": [false],
    "jsx-no-multiline-js": [false],
    "quotemark": [true, "single", "jsx-double"],
    "no-console": false,
    "object-literal-sort-keys": [true, "ignore-case"]
  },
  "rulesDirectory": []
}
